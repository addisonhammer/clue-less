<!doctype html>
<div style="text-align:center">
<title>Clue-Less</title>
<h1>Who done it?</h1>
<form name=room-form id=room-form class="form-inline" method="POST">
    <label>Select your room:</label>
    <select name="character" id="character" form_id=character-form>
      {% for room in rooms %}
          <option value="{{ room }}">{{ room }}</option>
      {% endfor %}
    </select>
    <input type="submit" value="Select Room">
</form>
</div>
